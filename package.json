{
  "name": "shaple-builder-chatbot",
  "version": "0.1.0",
  "private": true,
  "license": "ISC",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build:next-prod": "cross-env NODE_ENV=production next build",
    "build:next-dev": "cross-env NODE_ENV=development next build",
    "build:next": "run-s build:next-prod build:next-dev",
    "clean:next": "rimraf .next",
    "dev:migrate": "dotenv -e .env.local ts-node src/migrate.run.ts",
    "dev:run": "next dev",
    "dev": "run-s dev:*",
    "build": "run-s build:next",
    "clean": "run-s clean:*",
    "start:next": "cross-env NODE_ENV=production next start",
    "start": "run-p start:*",
    "docker:build": "docker build -t icn.ocir.io/axqafkgb0llg/shaple-builder-web:latest . --platform=linux/amd64",
    "docker:push": "docker push icn.ocir.io/axqafkgb0llg/shaple-builder-web:latest",
    "docker": "run-s docker:*",
    "local:setup:kind": "cd stoacloud && make setup-test",
    "local:setup:stoacloud:build": "cd stoacloud && docker build -t stoacloud/stoacloud:latest . && kind load docker-image stoacloud/stoacloud:latest --name kind --verbosity 1",
    "local:setup:stoacloud:dev:kubeconfig": "bash testdata/k8s/stoacloud/dev/kube-configmap.yaml.sh",
    "local:setup:stoacloud:test:kubeconfig": "bash testdata/k8s/stoacloud/test/kube-configmap.yaml.sh",
    "local:setup:stoacloud:dev:apply": "kubectl --context kind-kind apply -f testdata/k8s/stoacloud/dev && kubectl wait --context kind-kind --namespace default --for=condition=ready pod --selector=app=shaple-builder --timeout=60s",
    "local:setup:stoacloud:test:apply": "kubectl --context kind-kind apply -f testdata/k8s/stoacloud/test && kubectl wait --context kind-kind --namespace default --for=condition=ready pod --selector=app=shaple-builder-test --timeout=60s",
    "local:setup:stoacloud:dev:rollout": "kubectl --context kind-kind rollout restart deploy/shaple-builder && kubectl wait --context kind-kind --namespace default --for=condition=ready pod --selector=app=shaple-builder --timeout=60s",
    "local:setup:stoacloud:test:rollout": "kubectl --context kind-kind rollout restart deploy/shaple-builder-test && kubectl wait --context kind-kind --namespace default --for=condition=ready pod --selector=app=shaple-builder-test --timeout=60s",
    "local:setup": "run-s local:setup:kind local:setup:stoacloud:build local:setup:stoacloud:*:kubeconfig local:setup:stoacloud:*:apply",
    "local:rollout": "run-s local:setup:stoacloud:build local:setup:stoacloud:*:rollout",
    "local:teardown": "run-s local:teardown:kind",
    "local:teardown:kind": "cd stoacloud && make teardown-test",
    "test": "run-s generate:prisma && jest -i",
    "format:prisma": "prisma format",
    "format:prettier": "prettier --write src",
    "format": "run-s format:*",
    "lint": "next lint && prettier --check src",
    "generate:prisma": "prisma format && prisma generate"
  },
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.12",
    "@grpc/grpc-js": "^1.10.1",
    "@grpc/proto-loader": "^0.7.10",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@next/env": "^14.1.2",
    "@octokit/core": "^6.1.2",
    "@prisma/client": "^5.14.0",
    "@shaple/auth-helpers-nextjs": "^0.0.7",
    "@shaple/auth-helpers-shared": "^0.0.10",
    "@shaple/shaple": "^0.2.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^5.40.0",
    "@trpc/client": "^11.0.0-rc.382",
    "@trpc/react-query": "^11.0.0-rc.382",
    "@trpc/server": "^11.0.0-rc.382",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.0.1",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "disposablestack": "^1.1.6",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "lodash": "^4.17.21",
    "next": "14.1.0",
    "npm-run-all": "^4.1.5",
    "openai": "^4.40.0",
    "pg": "^8.11.5",
    "prisma": "^5.14.0",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "reflect-metadata": "^0.2.1",
    "remark-gfm": "^4.0.0",
    "rimraf": "^5.0.5",
    "superjson": "1.13.3",
    "swagger-ui-react": "^5.17.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typedi": "^0.10.0",
    "typescript": "^5.2",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@octokit/types": "^13.5.0",
    "@tanstack/react-query-devtools": "^5.37.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20",
    "@types/pg": "^8.11.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/swagger-ui-react": "^4.18.3",
    "@types/uuid": "^9.0.8",
    "@types/winston": "^2.4.4",
    "babel-jest": "^29.7.0",
    "eslint": "^7.23.0 || ^8.0.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "jsdom": "^24.0.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.2"
  }
}
