{
  "name": "shaple-builder-chatbot",
  "version": "0.1.0",
  "private": true,
  "license": "ISC",
  "scripts": {
    "build:proto": "proto-loader-gen-types --longs=Number --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=src/proto/ src/proto/*.proto",
    "build:next-prod": "cross-env APP_ENV=prod next build",
    "build:next-dev": "cross-env APP_ENV=dev next build",
    "build:next": "run-p build:next-prod build:next-dev",
    "build:wss": "tsc --project tsconfig.wss.json",
    "clean:proto": "rimraf src/proto/*.ts && rimraf src/proto/shaple/*.ts",
    "clean:wss": "rimraf dist",
    "clean:next": "rimraf .next",
    "dev:next": "cross-env APP_ENV=dev next dev",
    "dev:wss": "cross-env PORT=3001 NODE_ENV=development tsx watch src/server/wss.ts --tsconfig tsconfig.wss.json ",
    "dev": "run-p dev:*",
    "proto": "run-s clean:proto build:proto",
    "build": "run-s build:proto build:next build:wss",
    "clean": "run-s clean:*",
    "start:next": "cross-env NODE_ENV=production next start",
    "start:wss": "cross-env PORT=3001 NODE_ENV=production tsx src/server/wss.ts --tsconfig tsconfig.wss.json",
    "start": "run-p start:*",
    "docker:build": "docker build -t icn.ocir.io/axqafkgb0llg/shaple-builder-web:latest . --platform=linux/amd64",
    "docker:push": "docker push icn.ocir.io/axqafkgb0llg/shaple-builder-web:latest",
    "docker": "run-s docker:*"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.1",
    "@grpc/proto-loader": "^0.7.10",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@next/env": "^14.1.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^4.35.3",
    "@trpc/client": "^10.45.1",
    "@trpc/react-query": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "autoprefixer": "^10.0.1",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "next": "14.1.0",
    "npm-run-all": "^4.1.5",
    "openai": "^4.28.0",
    "pg-boss": "^9.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "reflect-metadata": "^0.2.1",
    "remark-gfm": "^4.0.0",
    "rimraf": "^5.0.5",
    "superjson": "^2.2.1",
    "tsx": "^4.7.1",
    "typedi": "^0.10.0",
    "typescript": "^5",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/ws": "^8.5.10",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0"
  }
}
