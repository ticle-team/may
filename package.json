{
  "name": "may",
  "version": "0.1.0",
  "private": true,
  "license": "ISC",
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/paust-team/may"
  },
  "scripts": {
    "clean:next": "rimraf .next",
    "dev:next": "ASSET_ID=v$npm_package_version next dev",
    "dev": "run-s generate dev:next",
    "build:next": "ASSET_ID=v$npm_package_version next build",
    "build": "run-s generate build:*",
    "clean": "run-s clean:*",
    "start": "next start",
    "docker:build": "docker build -t icn.ocir.io/axqafkgb0llg/may:latest . --platform=linux/amd64",
    "docker:push": "docker push icn.ocir.io/axqafkgb0llg/may:latest",
    "docker": "cp -rf $HOME/.aws ./ && run-s docker:*",
    "asset:push:dev": "aws s3 cp .next/static s3://cdn1.shaple.io/may/dev/v$npm_package_version/_next/static --recursive --acl public-read",
    "asset:push:prod": "aws s3 cp .next/static s3://cdn1.shaple.io/may/prod/v$npm_package_version/_next/static --recursive --acl public-read",
    "setup:kind": "cd stoacloud && make setup-test",
    "setup:stoacloud:build": "cd stoacloud && docker build --rm -t stoacloud/stoacloud:latest . && kind load docker-image stoacloud/stoacloud:latest --name kind --verbosity 1",
    "setup:stoacloud:dev": "bash testdata/k8s/stoacloud/dev/kube-configmap.yaml.sh && kubectl --context kind-kind -n default apply -f testdata/k8s/stoacloud/dev && kubectl wait --context kind-kind --namespace default --for=condition=ready pod --selector=app=shaple-builder --timeout=60s",
    "setup:stoacloud:test": "bash testdata/k8s/stoacloud/test/kube-configmap.yaml.sh && kubectl --context kind-kind -n default apply -f testdata/k8s/stoacloud/test && kubectl wait --context kind-kind --namespace default --for=condition=ready pod --selector=app=shaple-builder-test --timeout=60s",
    "setup:stoacloud": "run-s setup:stoacloud:*",
    "setup": "run-s setup:*",
    "teardown:stoacloud:dev": "kubectl --context kind-kind -n default delete -f testdata/k8s/stoacloud/dev",
    "teardown:stoacloud:test": "kubectl --context kind-kind -n default delete -f testdata/k8s/stoacloud/test",
    "teardown:stoacloud": "run-s teardown:stoacloud:*",
    "teardown:kind": "cd stoacloud && make teardown-test",
    "teardown": "run-s teardown:kind",
    "test:before": "run-s generate",
    "test:jest": "jest -i",
    "test": "run-s test:*",
    "format:prisma": "prisma format",
    "format:prettier": "prettier --write src",
    "format": "run-s format:*",
    "lint": "next lint && prettier --check src",
    "generate:prisma": "prisma format && prisma generate",
    "generate:proto:ts": "cd src/protos && grpc_tools_node_protoc --ts_out=. --ts_opt=unary_rpc_promise=true --ts_opt=json_names --ts_opt=target=node *.proto",
    "generate:proto:prettier": "prettier --write src/protos",
    "generate:proto": "run-s generate:proto:*",
    "generate": "run-s generate:*"
  },
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.12",
    "@grpc/grpc-js": "^1.10.1",
    "@grpc/proto-loader": "^0.7.10",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@langchain/azure-openai": "^0.0.11",
    "@langchain/openai": "^0.2.1",
    "@next/env": "^14.1.2",
    "@octokit/core": "^6.1.2",
    "@prisma/client": "^5.14.0",
    "@shaple/auth-helpers-nextjs": "^0.0.7",
    "@shaple/auth-helpers-shared": "^0.0.10",
    "@shaple/shaple": "^0.2.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^5.40.0",
    "@trpc/client": "^11.0.0-rc.382",
    "@trpc/react-query": "^11.0.0-rc.382",
    "@trpc/server": "^11.0.0-rc.382",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.0.1",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "disposablestack": "^1.1.6",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "flowbite-react": "^0.10.1",
    "google-protobuf": "^3.21.2",
    "langchain": "^0.2.8",
    "lodash-es": "^4.17.21",
    "next": "14.1.2",
    "npm-run-all": "^4.1.5",
    "openai": "^4.40.0",
    "pg": "^8.11.5",
    "prisma": "^5.14.0",
    "qs": "^6.12.1",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "reflect-metadata": "^0.2.1",
    "remark-gfm": "^4.0.0",
    "rimraf": "^5.0.5",
    "sharp": "^0.33.4",
    "superjson": "1.13.3",
    "swagger-ui-react": "^5.17.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typedi": "^0.10.0",
    "typescript": "^5.2",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@octokit/types": "^13.5.0",
    "@tanstack/react-query-devtools": "^5.37.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/pg": "^8.11.6",
    "@types/qs": "^6.9.15",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/swagger-ui-react": "^4.18.3",
    "@types/uuid": "^9.0.8",
    "@types/winston": "^2.4.4",
    "babel-jest": "^29.7.0",
    "eslint": "^7.23.0 || ^8.0.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "grpc-tools": "^1.12.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "jsdom": "^24.0.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "protoc-gen-ts": "^0.8.7",
    "semantic-release": "^24.0.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.2"
  },
  "sideEffects": false
}
