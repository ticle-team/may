{
  "name": "shaple-builder-chatbot",
  "version": "0.1.0",
  "private": true,
  "license": "ISC",
  "scripts": {
    "dev:next": "next dev",
    "dev:wss": "tsx watch src/server/wssDevServer.ts --tsconfig tsconfig.server.json ",
    "build:1-next": "cross-env NODE_ENV=production next build",
    "build:2-server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "build:proto": "proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=src/proto/ src/proto/*.proto",
    "clean:proto": "rimraf src/proto/*.ts && rimraf src/proto/shaple/*.ts",
    "proto": "run-s clean:proto build:proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.1",
    "@grpc/proto-loader": "^0.7.10",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^4.35.3",
    "@trpc/client": "^10.45.1",
    "@trpc/react-query": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "next": "14.1.0",
    "openai": "^4.28.0",
    "pg-boss": "^9.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "reflect-metadata": "^0.2.1",
    "remark-gfm": "^4.0.0",
    "superjson": "^2.2.1",
    "typedi": "^0.10.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.7.1",
    "typescript": "^5"
  }
}
